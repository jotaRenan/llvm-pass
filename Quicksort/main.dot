digraph "CFG for 'main' function" {
	entry[shape=record,
		label="{entry:\l\l
			br for.cond \l
		}"
	];
	entry -> forcond;
	forcond[shape=record,
		label="{for.cond:\l\l
			%66 = load i \l
			cmp = icmp %66 100 \l
			br cmp for.end for.body \l
		}"
	];
	forcond -> forbody;
	forcond -> forend;
	forbody[shape=record,
		label="{for.body:\l\l
			%67 = load i \l
			call %67 Quick \l
			br for.inc \l
		}"
	];
	forbody -> forinc;
	forinc[shape=record,
		label="{for.inc:\l\l
			%68 = load i \l
			inc = add %68 1 \l
			br for.cond \l
		}"
	];
	forinc -> forcond;
	forend[shape=record,
		label="{for.end:\l\l
			ret 0 \l
		}"
	];
}
